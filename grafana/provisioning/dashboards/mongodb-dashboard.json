    {
      "id": 21,
      "title": "Vector Search Serialization Performance",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(mongot_command_vectorSearchCommandSerializationLatency_seconds_count[$__rate_interval])",
          "refId": "A",
          "legendFormat": "Serialization Operations/sec",
          "datasource": "Prometheus"
        },
        {
          "expr": "rate(mongot_command_vectorSearchCommandSerializationLatency_seconds_sum[$__rate_interval]) / rate(mongot_command_vectorSearchCommandSerializationLatency_seconds_count[$__rate_interval])",
          "refId": "B",
