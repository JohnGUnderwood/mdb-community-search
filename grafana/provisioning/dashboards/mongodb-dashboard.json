{
  "id": null,
  "title": "MongoDB Community Search Monitoring",
  "tags": ["mongodb", "search", "mongot"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "5s",
  "schemaVersion": 27,
  "version": 2,
  "panels": [
    {
      "id": 1,
      "title": "Service Status",
      "type": "stat",
      "targets": [
        {
          "expr": "mongodb_up",
          "refId": "A",
          "legendFormat": "MongoDB",
          "datasource": "Prometheus"
        },
        {
          "expr": "up{job=\"mongot\"}",
          "refId": "B", 
          "legendFormat": "Mongot",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "green", "value": 1}
            ]
          },
          "mappings": [
            {"options": {"0": {"text": "DOWN"}}, "type": "value"},
            {"options": {"1": {"text": "UP"}}, "type": "value"}
          ]
        }
      },
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "MongoDB Connections",
      "type": "timeseries",
      "targets": [
        {
          "expr": "mongodb_ss_connections{conn_type=\"current\"}",
          "refId": "A",
          "legendFormat": "Current",
          "datasource": "Prometheus"
        },
        {
          "expr": "mongodb_ss_connections{conn_type=\"active\"}",
          "refId": "B",
          "legendFormat": "Active",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short",
          "min": 0
        }
      },
      "gridPos": {"h": 6, "w": 9, "x": 6, "y": 0}
    },
    {
      "id": 3,
      "title": "MongoDB Operations/sec",
      "type": "timeseries", 
      "targets": [
        {
          "expr": "rate(mongodb_ss_opcounters{legacy_op_type=\"insert\"}[$__rate_interval])",
          "refId": "A",
          "legendFormat": "Inserts",
          "datasource": "Prometheus"
        },
        {
          "expr": "rate(mongodb_ss_opcounters{legacy_op_type=\"query\"}[$__rate_interval])",
          "refId": "B",
          "legendFormat": "Queries",
          "datasource": "Prometheus"
        },
        {
          "expr": "rate(mongodb_ss_opcounters{legacy_op_type=\"update\"}[$__rate_interval])",
          "refId": "C",
          "legendFormat": "Updates",
          "datasource": "Prometheus"
        },
        {
          "expr": "rate(mongodb_ss_opcounters{legacy_op_type=\"delete\"}[$__rate_interval])",
          "refId": "D",
          "legendFormat": "Deletes",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "ops",
          "min": 0
        }
      },
      "gridPos": {"h": 6, "w": 9, "x": 15, "y": 0}
    },
    {
      "id": 4,
      "title": "MongoDB Memory Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "mongodb_ss_mem_resident * 1024 * 1024",
          "refId": "A",
          "legendFormat": "Resident Memory",
          "datasource": "Prometheus"
        },
        {
          "expr": "mongodb_ss_mem_virtual * 1024 * 1024",
          "refId": "B",
          "legendFormat": "Virtual Memory",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "bytes",
          "min": 0
        }
      },
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 6}
    },
    {
      "id": 5,
      "title": "Network I/O",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(mongodb_ss_network_bytesIn[$__rate_interval])",
          "refId": "A",
          "legendFormat": "Bytes In/sec",
          "datasource": "Prometheus"
        },
        {
          "expr": "rate(mongodb_ss_network_bytesOut[$__rate_interval])",
          "refId": "B",
          "legendFormat": "Bytes Out/sec",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "Bps",
          "min": 0
        }
      },
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 6}
    },
    {
      "id": 6,
      "title": "Search Command Latency",
      "type": "timeseries",
      "targets": [
        {
          "expr": "mongot_command_searchCommandTotalLatency_seconds",
          "refId": "A",
          "legendFormat": "Query {{quantile}}th percentile",
          "datasource": "Prometheus"
        },
        {
          "expr": "mongot_command_indexStatsCommandTotalLatency_seconds",
          "refId": "B",
          "legendFormat": "Index {{quantile}}th percentile",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "s",
          "min": 0
        }
      },
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 12}
    },
    {
      "id": 7,
      "title": "Search Command Rates",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(mongot_command_searchCommandTotalLatency_seconds_count[$__rate_interval])",
          "refId": "A", 
          "legendFormat": "Search Commands/sec",
          "datasource": "Prometheus"
        },
        {
          "expr": "rate(mongot_command_indexStatsCommandTotalLatency_seconds_count[$__rate_interval])",
          "refId": "B",
          "legendFormat": "Index Stats Commands/sec",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "ops",
          "min": 0
        }
      },
      "gridPos": {"h": 6, "w": 8, "x": 8, "y": 12}
    },
    {
      "id": 8,
      "title": "Search Command Failures",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(mongot_command_searchCommandFailure_total[$__rate_interval])",
          "refId": "A",
          "legendFormat": "Search Failures/sec",
          "datasource": "Prometheus"
        },
        {
          "expr": "rate(mongot_command_indexStatsCommandFailure_total[$__rate_interval])",
          "refId": "B",
          "legendFormat": "Index Stats Failures/sec",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "ops",
          "min": 0
        }
      },
      "gridPos": {"h": 6, "w": 8, "x": 16, "y": 12}
    },
    {
      "id": 9,
      "title": "Search Index Count",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(mongot_configState_indexesInCatalog)",
          "refId": "A",
          "legendFormat": "Total Indexes",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "unit": "short",
          "noValue": "No Indexes",
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "yellow", "value": 1},
              {"color": "green", "value": 2}
            ]
          }
        }
      },
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 18}
    },
    {
      "id": 10,
      "title": "Search Index Throughput",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(mongot_index_stats_indexing_insert_total[$__rate_interval])",
          "refId": "A",
          "legendFormat": "Inserts/sec - {{indexId_logString}}",
          "datasource": "Prometheus"
        },
        {
          "expr": "rate(mongot_index_stats_indexing_update_total[$__rate_interval])",
          "refId": "B",
          "legendFormat": "Updates/sec - {{indexId_logString}}",
          "datasource": "Prometheus"
        },
        {
          "expr": "rate(mongot_index_stats_indexing_delete_total[$__rate_interval])",
          "refId": "C",
          "legendFormat": "Deletes/sec - {{indexId_logString}}",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "ops",
          "min": 0
        }
      },
      "gridPos": {"h": 6, "w": 12, "x": 6, "y": 18}
    },
    {
      "id": 11,
      "title": "Search Index Replication Lag",
      "type": "timeseries",
      "targets": [
        {
          "expr": "mongot_index_stats_indexing_replicationLagMs",
          "refId": "A",
          "legendFormat": "Replication Lag - {{indexId_logString}}",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "ms",
          "min": 0,
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 1000},
              {"color": "red", "value": 5000}
            ]
          }
        }
      },
      "gridPos": {"h": 6, "w": 6, "x": 18, "y": 18}
    },
    {
      "id": 12,
      "title": "Index Size and Documents",
      "type": "timeseries",
      "targets": [
        {
          "expr": "mongot_index_stats_indexSizeBytes / 1024 / 1024",
          "refId": "A",
          "legendFormat": "Index Size MB - {{indexId_logString}}",
          "datasource": "Prometheus"
        },
        {
          "expr": "mongot_index_stats_numLuceneDocs",
          "refId": "B",
          "legendFormat": "Lucene Docs - {{indexId_logString}}",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short",
          "min": 0
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Index Size MB"},
            "properties": [{"id": "unit", "value": "decmbytes"}]
          }
        ]
      },
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 24}
    },
    {
      "id": 25,
      "title": "Average Search Latency",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(mongot_command_searchCommandTotalLatency_seconds_sum[$__rate_interval]) / rate(mongot_command_searchCommandTotalLatency_seconds_count[$__rate_interval])",
          "refId": "A",
          "legendFormat": "Avg Search Latency",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "unit": "s",
          "noValue": "No Search Data",
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 0.1},
              {"color": "red", "value": 1}
            ]
          }
        }
      },
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 30}
    },
    {
      "id": 26,
      "title": "Average Vector Search Latency",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(mongot_command_vectorSearchCommandTotalLatency_seconds_sum[$__rate_interval]) / rate(mongot_command_vectorSearchCommandTotalLatency_seconds_count[$__rate_interval])",
          "refId": "A",
          "legendFormat": "Avg Vector Search Latency",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "unit": "s",
          "noValue": "No Vector Search Data",
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 0.1},
              {"color": "red", "value": 1}
            ]
          }
        }
      },
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 30}
    },
    {
      "id": 14,
      "title": "Indexing Batch Performance",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(mongot_index_stats_indexing_indexingBatchDurations_seconds_sum[$__rate_interval]) / rate(mongot_index_stats_indexing_indexingBatchDurations_seconds_count[$__rate_interval])",
          "refId": "A",
          "legendFormat": "Avg Batch Duration - {{indexId_logString}}",
          "datasource": "Prometheus"
        },
        {
          "expr": "rate(mongot_index_stats_indexing_totalBytesProcessed_total[$__rate_interval])",
          "refId": "B",
          "legendFormat": "Bytes Processed/sec - {{indexId_logString}}",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "s",
          "min": 0
        },
        "overrides": [
          {
            "matcher": {"id": "byRegexp", "options": "/Bytes.*/"}, 
            "properties": [{"id": "unit", "value": "Bps"}]
          }
        ]
      },
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 36}
    },
    {
      "id": 15,
      "title": "Query Feature Usage Rate by Operator Type",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(mongot_index_stats_query_feature_total[$__rate_interval]) * 60",
          "refId": "A",
          "legendFormat": "{{name}}",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "qpm",
          "min": 0,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 1,
            "fillOpacity": 0,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "axisColorMode": "text",
            "scaleDistribution": {
              "type": "linear"
            },
            "axisCenteredZero": false,
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "lastNotNull"
          ],
          "values": [
            "value"
          ]
        }
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 36}
    },
    {
      "id": 16,
      "title": "Vector Search numCandidates Distribution",
      "type": "timeseries",
      "targets": [
        {
          "expr": "mongot_index_stats_query_numCandidatesPerQuery{quantile=\"0.5\"}",
          "refId": "A",
          "legendFormat": "p50 Candidates - {{indexId_logString}}",
          "datasource": "Prometheus"
        },
        {
          "expr": "mongot_index_stats_query_numCandidatesPerQuery{quantile=\"0.95\"}",
          "refId": "B",
          "legendFormat": "p95 Candidates - {{indexId_logString}}",
          "datasource": "Prometheus"
        },
        {
          "expr": "mongot_index_stats_query_numCandidatesPerQuery{quantile=\"0.99\"}",
          "refId": "C",
          "legendFormat": "p99 Candidates - {{indexId_logString}}",
          "datasource": "Prometheus"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short",
          "min": 0,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 1,
            "fillOpacity": 0,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "Documents",
            "axisColorMode": "text",
            "scaleDistribution": {
              "type": "linear"
            },
            "axisCenteredZero": false,
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 44}
    },
    {
      "id": 17,
      "title": "Vector Search Limit Distribution",
