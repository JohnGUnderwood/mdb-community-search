    {
      "id": 19,
      "title": "Vector Search Command Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(mongot_command_vectorSearchCommandTotalLatency_seconds_count[$__rate_interval])",
          "refId": "A",
          "legendFormat": "Vector Search Commands/sec",
