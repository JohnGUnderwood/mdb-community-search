    {
      "id": 20,
      "title": "Vector Search Command Latency Distribution",
      "type": "timeseries",
      "targets": [
        {
          "expr": "mongot_command_vectorSearchCommandTotalLatency_seconds{quantile=\"0.5\"}",
          "refId": "A",
          "legendFormat": "p50 Vector Search Latency",
