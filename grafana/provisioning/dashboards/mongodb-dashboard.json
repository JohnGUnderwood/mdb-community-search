            "axisColorMode": "text",
            "scaleDistribution": {
              "type": "linear"
            },
            "axisCenteredZero": false,
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "gridPos": {"h": 6, "w": 8, "x": 8, "y": 44}
    },
    {
      "id": 18,
      "title": "Search Query Batch Data Size Distribution",
      "type": "timeseries",
      "targets": [
        {
          "expr": "mongot_index_stats_query_batchDataSize{quantile=\"0.5\"}",
